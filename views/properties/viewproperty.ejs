      <%-}); %>
  </tr>
  <tr>
      <td>electricity</td>
      <%- transactions.forEach(function (element) {
        %> <td> <%= element.electricity %> </td>
      <%-});%>
  </tr>
  <tr>
      <td>appartment-construction</td>
      <%- transactions.forEach(function (element) {
        %> <td> <%= element.appartment-construction %> </td>
      <%-});%>
  </tr>
  <tr>
      <td>wifi</td>
      <%- transactions.forEach(function (element) {
        %> <td> <%= element.wifi %> </td>
      <%-});%>
  </tr>
  <tr>
      <td>community</td>
      <%- transactions.forEach(function (element) {
        %> <td> <%= element.community %> </td>
      <%-});%>
  </tr>
  <tr>
      <td>general-maintenance</td>
      <%- transactions.forEach(function (element) {
        %> <td> <%= element.general-maintenance %> </td>
      <%-});%>
  </tr>
</table>