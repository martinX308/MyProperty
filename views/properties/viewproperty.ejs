<h1>Your property details</h1>

<table >
  <caption>Monthly balance </caption>
  <tr>
    <th></th>
    <%- transactions.forEach(function (element) {
     %> <th> <%= element.month %> </th>
    <%-}); %>
  </tr>
  <tr>
    <td>rent</td>
    <%- transactions.forEach(function (element) {
      %> <td> <%= element.rent %> </td>
    <%-}); %>
  </tr>
  <tr>
      <td>tentantFee</td>
      <%- transactions.forEach(function (element) {
        %> <td> <%= element.tentantFee %> </td>
      <%-}); %>
  </tr>
